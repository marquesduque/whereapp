@model api.Models.filial

@{
    ViewBag.Title = "Edit";
}

<section class="content-header">
    <h1>
        <a href="@Url.Action("Index")" class="btn btn-warning"><i class="fa fa-angle-left fa-lg" style="margin-right: 10px"></i>Voltar</a>
        <a href="#" class="btn btn-danger" base_library="api.models.filial.library_create" onclick="reiniciar()"><i class="fa fa-plus" style="margin-right:10px"></i>REINICIAR APP</a>
        <script>
            function reiniciar() {
                $('[name="versao"]').val(Math.floor((Math.random() * 10) + 1));
                $('form').submit();
            }
        </script>
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("index")" base_library="api.Models.filial.library_bc_1">FILIAL</a></li>
        <li class="active" base_library="api.Models.filial.library_bc_edit">EDITAR</li>
    </ol>
</section>


<section class="content">

    <div style="clear: both"></div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken();
        @Html.ValidationSummary(true)
        <div class="alert alert-danger" role="alert" base_library="api.Models.filial.library_error_register">
            Registro não gravado, verificar erro criticado
        </div>


        <fieldset>

            @*<div class="col-md-12 bar">
            <i class="fa fa-pencil" style="margin-right: 10px"></i><b base_library="api.Models.filial.library_code">Editando Código:</b> <span class="editid">@Model.id</span>
        </div>*@
            @Html.HiddenFor(model => model.id)


            <div class="alert bg-gray" role="alert" style="padding: 20px 8px 20px 25px;">

                <h1 style="FONT-WEIGHT: 700;">Dados da Filial</h1>
                <div>Informe abaixo os dados da Filial</div>
            </div>

            <div class="form-group col-md-12">
                <label class="control-label" base_library="api.Models.filial.nome">Nome da Filial</label>

                <div style="clear: both"></div>
                @Html.TextBoxFor(model => model.nome, new { placeholder = "", @class = "form-control", maxLength = "150" })
                @Html.ValidationMessageFor(model => model.nome)
            </div>


            <div class="form-group col-md-3">
                <label class="control-label" base_library="api.Models.filial.telefone">Telefone<div style="text-transform:uppercase; font-size:10px">Informe o código do pais e DDD</div></label>

                <div style="clear: both"></div>
                @Html.TextBoxFor(model => model.telefone, new { placeholder = "", @class = "form-control", maxLength = "150", mask = "{\"value\":\"+999999999999\"}" })
                @Html.ValidationMessageFor(model => model.telefone)
            </div>



            <div class="form-group col-md-3">
                <label class="control-label" base_library="api.Models.filial.celular">Whatsapp<div style="text-transform:uppercase;font-size:10px">Informe o código do pais e DDD</div></label>

                <div style="clear: both"></div>
                @Html.TextBoxFor(model => model.celular, new { placeholder = "", @class = "form-control", maxLength = "150", mask = "{\"value\":\"+9999999999999\"}" })
                @Html.ValidationMessageFor(model => model.celular)
            </div>

            <div class="form-group col-md-3">
                <label class="control-label" base_library="api.Models.filial.tempoentrega">Tempo médio (min)<div style="text-transform:uppercase;font-size:10px">para retirada ou entrega</div></label>
                <div style="clear: both"></div>
                @Html.TextBoxFor(model => model.tempoentrega, new { placeholder = "", @class = "form-control", maxLength = "150", type = "number", min = "1", max = "50" })
                @Html.ValidationMessageFor(model => model.tempoentrega)
            </div>
            <div class="form-group col-md-3">
                <label class="control-label" base_library="api.Models.filial.minimo">Valor mínimo<div style="text-transform:uppercase;font-size:10px">para finaliza um pedido</div></label>

                <div style="clear: both"></div>
                @Html.TextBoxFor(model => model.minimo, new { placeholder = "", @class = "form-control", maxLength = "150", mask = "money" })
                @Html.ValidationMessageFor(model => model.minimo)
            </div>

            <div class="alert bg-blue col-md-12" role="alert" style="padding: 20px 8px; margin-top:20px">

                <div style="padding:0px 20px 20px 20px">
                    @if (Model.BankCode == null)
                    {
                        <h1 style="FONT-WEIGHT: 700;">Localidade da filial</h1>
                        <div>O APP É GEOLOCAIZADO INFORME ABAIXO O LOCAL EXATO DO SEU ESTABELECIMENTO</div>
                    }
                </div>

                <div class="form-group col-md-12">
                    <label class="control-label" base_library="api.Models.montagem.buscar">
                        Buscar Endereço
                        <div style="font-size:10px">BUSQUE E SELECIONE O ENDEREÇO PARA CONTINUAR</div>
                    </label>
                    <div style="clear: both"></div>
                    @Html.TextBoxFor(model => model.address, new { placeholder = "Busca Endereço", @class = "form-control", maxLength = "150", type = "text", id = "pac-input", })
                    @Html.ValidationMessageFor(model => model.address)
                </div>

                <div class="form-group col-md-4">
                    <label class="control-label" base_library="api.Models.filial.endereco">Endereço</label>
                    <div style="clear: both"></div>
                    @Html.TextBoxFor(model => model.endereco, new { placeholder = "", onclick = "modal('Para informar o endereço realize uma busca no campo acima \"Buscar endereço\" e selecione um logradouro.')", @class = "form-control", maxLength = "150", type = "text", Readonly = "readonly" })
                    @Html.ValidationMessageFor(model => model.endereco)
                </div>
                <div class="form-group col-md-1">
                    <label class="control-label" base_library="api.Models.filial.numero">Número</label>

                    <div style="clear: both"></div>
                    @Html.TextBoxFor(model => model.numero, new { placeholder = "", @class = "form-control", maxLength = "150" })
                    @Html.ValidationMessageFor(model => model.numero)
                </div>
                <div class="form-group col-md-1">
                    <label class="control-label" base_library="api.Models.filial.estado">UF</label>

                    <div style="clear: both"></div>
                    @Html.TextBoxFor(model => model.estado, new { placeholder = "", onclick = "modal('Para informar o endereço realize uma busca no campo acima \"Buscar endereço\" e selecione um logradouro.')", @class = "form-control", maxLength = "150", Readonly = "readonly" })
                    @Html.ValidationMessageFor(model => model.estado)
                </div>
                <div class="form-group col-md-2">
                    <label class="control-label" base_library="api.Models.filial.cidade">Cidade</label>

                    <div style="clear: both"></div>
                    @Html.TextBoxFor(model => model.cidade, new { placeholder = "", onclick = "modal('Para informar o endereço realize uma busca no campo acima \"Buscar endereço\" e selecione um logradouro.')", @class = "form-control", maxLength = "150", Readonly = "readonly" })
                    @Html.ValidationMessageFor(model => model.cidade)
                </div>


                @Html.HiddenFor(model => model.pais, new { placeholder = "", @class = "form-control", maxLength = "150", Readonly = "readonly" })
                @Html.ValidationMessageFor(model => model.pais)

                <div class="form-group col-md-4">
                    <label class="control-label" base_library="api.Models.filial.complemento">complemento</label>

                    <div style="clear: both"></div>
                    @Html.TextBoxFor(model => model.complemento, new { placeholder = "", @class = "form-control", maxLength = "150" })
                    @Html.ValidationMessageFor(model => model.complemento)
                </div>

            </div>
            <div class="alert bg-red col-md-12" role="alert" style="padding: 20px 8px;">

                <div style="padding:0px 20px 20px 20px" class="col-md-12">

                    <h1 style="FONT-WEIGHT: 700;">Configuração de entrega</h1>
                    <div>Informe abaixo o modelo de entrega adequado para a filial</div>

                </div>

                <div style="clear:both"></div>

                <div style="clear:both"></div>
                <div class="form-group  col-md-6">
                    <div class="alert bg-gray" role="alert" style="padding: 20px;">
                        <label class="control-label" base_library="api.Models.filial.Balcao">
                            Retirar no local
                            <div style="font-size:10px;FONT-WEIGHT: normal;">O CLIENTE FARA A RETIRADA DO PRODUTO NO ESTABELECIMENTO</div>
                        </label>
                        <div style="height: 34px; line-height: 30px;">
                            <label style="min-width: 45px;">
                                <input type="radio" value="true" name="item_correctt" class="minimal-green">
                                ATIVAR

                            </label>
                            <label style="min-width: 45px;">
                                <input type="radio" value="false" name="item_correctt" class="minimal">
                                DESATIVAR

                            </label>
                            @Html.HiddenFor(model => model.balcao, new { icheck = "item_correctt" })
                            @Html.ValidationMessageFor(model => model.balcao)
                        </div>
                    </div>
                </div>

                <div class="form-group  col-md-6">
                    <div class="alert bg-gray" role="alert" style="padding: 20px;">
                        <label class="control-label" base_library="api.Models.filial.Delivery">
                            Delivery
                            <div style="font-size:10px;FONT-WEIGHT: normal;">O CLIENTE RECEBERÁ O PEDIDO NO ENDEREÇO ESCOLHIDO</div>
                        </label>
                        <div style="height: 34px; line-height: 30px;">
                            <label style="min-width: 45px;">
                                <input type="radio" value="true" name="item_correct" class="minimal-green">
                                ATIVAR

                            </label>
                            <label style="min-width: 45px;">
                                <input type="radio" value="false" name="item_correct" class="minimal">
                                DESATIVAR

                            </label>
                            @Html.HiddenFor(model => model.delivery, new { icheck = "item_correct", callback = "display_delivery" })
                            @Html.ValidationMessageFor(model => model.delivery)

                            <script>
                                function display_delivery(result) {
                                    if (result == 'true') {
                                        $('#display_delivery').show();
                                    }
                                    else {
                                        $('#display_delivery').hide();
                                    }
                                }
                            </script>
                        </div>
                    </div>
                </div>
                <div style="clear:both"></div>
                <div id="display_delivery" style="@(Model.delivery == true ? "": "display:none")">
                    <div style="clear:both"></div>
                    <div style="padding:0px 15px 15px 15px">
                        <ul class="nav nav-tabs">
                            <li class="@(Model.entrega_distancia_maxima == null ? "" : "active")">
                                <a style="text-decoration:none" onclick="$('#pnl_distancia').tab('show'); $('#pnl_desenhar_mapa').removeClass('active'); $('.nav.nav-tabs .active').removeClass('active'); $(this).closest('li').addClass('active')">Distância Maxima</a>


                            </li>
                            <li class="@(string.IsNullOrEmpty(Model.entraga_cep_bloqueado) ? "" : "active")">
                                <a style="text-decoration:none" onclick="$('#pnl_desenhar_mapa').tab('show'); $('#pnl_distancia').removeClass('active'); $('.nav.nav-tabs .active').removeClass('active'); $(this).closest('li').addClass('active')">Desenhar no MAPA</a>

                            </li>
                        </ul>
                        <i class="glyphicon glyphicon-arrow-down"
                           style="    font-size: 30px;
    position: relative;
    z-index: 9999999;
    margin-top: -75px;
    color: #FFC107;
    text-shadow: 1p;
    text-shadow: 1px 1px rgba(0, 0, 0, 0.60);
    float: right;
    margin-right: 2px;"></i>
                        <div class="tab-content">
                            <div id="pnl_distancia" class="tab-pane fade in @(Model.entrega_distancia_maxima == null ? "" : "active")">
                                <div class="form-group">
                                    <label class="control-label" base_library="api.Models.filial.entrega_distancia_maxima">
                                        DISTANCIA MÁXIMA QUE A ENTREGA PODERA SER RELIZADA
                                    </label>

                                    <div style="clear: both"></div>
                                    <input name="entrega_distancia_maxima" onKeyPress="if (this.value.length == 6 || event.keyCode < 48 || event.keyCode > 57) { return false; }" type="number" inputmode="numeric" pattern="[0-9]*" class="form-control" value="@Model.entrega_distancia_maxima" placeholder="Distância em Kms" />

                                    @Html.ValidationMessageFor(model => model.entrega_distancia_maxima)
                                </div>
                            </div>
                            <div id="pnl_desenhar_mapa" class="tab-pane fade in @(string.IsNullOrEmpty(Model.entraga_cep_bloqueado) ? "" : "active")">
                                <div class="form-group">
                                    <label class="control-label" base_library="api.Models.filial.entrega_distancia_maxima">
                                        RECORTE NO MAPA A REGIÃO DE ATENDIMENTO PARA ENTREGA
                                    </label>

                                    <div style="clear: both"></div>
                                    <a href="@Url.Action("details", new { id = Model.id })" style="width:100%; text-decoration:none;    line-height: 0px;" class="btn btn-info" base_library="api.models.filial.library_map"><i class="fa fa-map" style="margin-right:10px"></i>Recortar Mapa</a>

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-group  col-md-12">
                        <div class="alert bg-gray" role="alert" style="padding: 20px;">

                            <div class="form-group col-md-6">
                                <label class="control-label" base_library="api.Models.filial.entrega_distancia_gratis">
                                    Taxa de entrega
                                    <div style="font-size:10px;FONT-WEIGHT: normal;">
                                        VARIAVEL: SERÁ CALCULADO O VALOR TOTAL EM KM E MULTIPLICADO PELO VALOR DA ENTREGA<br />
                                        FIXO: SERA COBRADO UM VALOR FIXO INFORMADO NO VALOR DA ENTREGA
                                    </div>
                                </label>
                                <div style="height: 34px; line-height: 30px;">
                                    <label style="min-width: 45px;">
                                        <input type="radio" value="0" name="item_entrega_distancia_gratis" class="minimal-green">
                                        Fixo

                                    </label>
                                    <label style="min-width: 45px;">
                                        <input type="radio" value="1" name="item_entrega_distancia_gratis" class="minimal">
                                        Variável
                                    </label>
                                    @Html.HiddenFor(model => model.entrega_distancia_gratis, new { icheck = "item_entrega_distancia_gratis" })
                                    @Html.ValidationMessageFor(model => model.entrega_distancia_gratis)

                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label" base_library="api.Models.filial.entraga_valor_km">
                                    Valor da entrega
                                    <div style="font-size:10px;FONT-WEIGHT: normal;">
                                        ATENÇÃO: INFORME O VALOR RELATIVO A TAXA DE ENTREGA ESCOLHIDA
                                    </div>
                                </label>

                                <div style="clear: both"></div>
                                @Html.TextBoxFor(model => model.entraga_valor_km, new { placeholder = "", @class = "form-control", maxLength = "150", mask = "money" })
                                @Html.ValidationMessageFor(model => model.entraga_valor_km)
                            </div>
                            <div style="clear:both"></div>
                        </div>
                    </div>
                </div>
            </div>


            @Html.HiddenFor(model => model.matriz_id)



            @Html.HiddenFor(model => model.administrador_id)



            @Html.HiddenFor(model => model.cor_id)




            @*Latitude e Longitude*@
            @Html.HiddenFor(model => model.lat)
            @Html.ValidationMessageFor(model => model.lat)

            @Html.HiddenFor(model => model.lng)
            @Html.ValidationMessageFor(model => model.lng)








            @Html.HiddenFor(model => model.versao, new { placeholder = "", @class = "form-control", maxLength = "150" })


            @Html.HiddenFor(model => model.connection_string, new { placeholder = "", @class = "form-control", maxLength = "5000" })





            @Html.HiddenFor(model => model.entraga_cep_bloqueado, new { placeholder = "", @class = "form-control", maxLength = "150" })

            <div style="clear:both"></div>
            <div class="alert bg-gray col-md-12" role="alert" style="padding: 20px 8px;">

                <div style="padding:0px 20px 20px 20px" class="col-md-12">

                    <h1 style="FONT-WEIGHT: 700;">Programação</h1>
                    <div>Informe abaixo se seu estabelecimento esta fechado ou aberto</div>

                </div>

                <div style="padding:20px;">
                    <div class="form-group col-md-2" style="min-width: 160px;">
                        <label class="control-label" base_library="api.Models.filial.Balcao">Aberto<div style="font-size:10px">NO MOMENTO?</div></label>
                        <div style="height: 34px; line-height: 30px;">
                            <label style="min-width: 45px;">
                                <input type="radio" value="true" name="item_aberto" class="minimal-green">
                                Sim

                            </label>
                            <label style="min-width: 45px;">
                                <input type="radio" value="false" name="item_aberto" class="minimal">
                                Não

                            </label>
                            @Html.HiddenFor(model => model.aberto, new { icheck = "item_aberto" })
                            @Html.ValidationMessageFor(model => model.aberto)
                        </div>
                    </div>

                    <div class="form-group col-md-10">
                        <label class="control-label" base_library="api.Models.montagem.data_fechado">Agendar fechamento<div style="font-size:10px">INFORME AS DATAS QUE O APP ESTARÁ FECHADO. EX: FERIÁDOS</div></label>
                        <select multiple="multiple" class="form-control select2" source="#data_fecahado" data-placeholder="" style="width: 100%;">
                            @for (int i = 0; i < 365; i++)
                            {
                                <option value="@DateTime.Now.AddDays(i).ToShortDateString()">@(DateTime.Now.AddDays(i).ToShortDateString())</option>
                            }
                        </select>
                        @Html.HiddenFor(model => model.data_fechado)
                        @Html.ValidationMessageFor(model => model.data_fechado)
                    </div>
                </div>
                <div style="padding:40px 20px 20px 20px" class="col-md-12">

                    <h1 style="FONT-WEIGHT: 700;">Horário de funcionamento</h1>
                    <div>Informe abaixo os horários e datas de abertura e fechamento do seu estabelecimento</div>

                </div>
                <style>
                    .horarios input {
                        line-height: 32px;
                        width: calc(100% - 85px);
                        float: left;
                    }

                    .horarios label {
                        float: left;
                        line-height: 35px;
                        margin-right: 10px;
                        width: 75px;
                </style>
                <div class="horarios">
                    <div class="col-md-12">
                        <div style="clear:both"></div>
                        <div class="semana">SEGUNDA</div>
                        <div style="clear:both"></div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.seg1">Abertura</label>
                            @Html.TextBoxFor(model => model.seg1, new { placeholder = "Manhã", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.seg1)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.seg2">Fechamento</label>
                            @Html.TextBoxFor(model => model.seg2, new { placeholder = "Manhã", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.seg2)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.seg3">Abertura</label>
                            @Html.TextBoxFor(model => model.seg3, new { placeholder = "Tarde", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.seg3)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.seg4">Fechamento</label>
                            @Html.TextBoxFor(model => model.seg4, new { placeholder = "Tarde", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.seg4)
                        </div>
                        <div style="clear:both"></div>
                    </div>

                    <div class="col-md-12">
                        <div class="semana">TERÇA</div>
                        <div style="clear:both"></div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.ter1">Abertura</label>
                            @Html.TextBoxFor(model => model.ter1, new { placeholder = "Manhã", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.ter1)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.ter2">Fechamento</label>
                            @Html.TextBoxFor(model => model.ter2, new { placeholder = "Manhã", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.ter2)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.ter3">Abertura</label>
                            @Html.TextBoxFor(model => model.ter3, new { placeholder = "Tarde", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.ter3)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.ter4">Fechamento</label>
                            @Html.TextBoxFor(model => model.ter4, new { placeholder = "Tarde", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.ter4)
                        </div>
                    </div>

                    <div class="col-md-12">
                        <div class="semana">QUARTA</div>
                        <div style="clear:both"></div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.qua1">Abertura</label>
                            @Html.TextBoxFor(model => model.qua1, new { placeholder = "Manhã", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.qua1)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.qua2">Fechamento</label>
                            @Html.TextBoxFor(model => model.qua2, new { placeholder = "Manhã", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.qua2)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.qua3">Abertura</label>
                            @Html.TextBoxFor(model => model.qua3, new { placeholder = "Tarde", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.qua3)
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label" base_library="api.Models.filial.qua4">Fechamento</label>
                            @Html.TextBoxFor(model => model.qua4, new { placeholder = "Tarde", @class = "form-control", maxLength = "150", mask = "{\"value\":\"99:99\", \"placeholder\":\"00:00\"}" })
                            @Html.ValidationMessageFor(model => model.qua4)
                        </div>
                    </div>

                    <div class="col-md-12">
                        <div class="semana">QUINTA</div>
                        <div style="clear:both"></div>
                        <div class="form-group col-md-3">
